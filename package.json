{
  "name": "kawajs",
  "version": "1.1.0",
  "description": "React/Redux framwork on caffeine",
  "author": "Paul Reboh",
  "license": "MIT",
  "keywords": [
    "react",
    "redux"
  ],
  "homepage": "https://github.com/somelibs/kawajs",
  "repository": {
    "type": "git",
    "url": "https://github.com/somelibs/kawajs.git"
  },
  "bugs": {
    "url": "https://github.com/somelibs/kawajs/issues"
  },
  "files": [
    "dist/cjs/**",
    "dist/es/**"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build:commonjs": "BABEL_ENV=commonjs babel src --out-dir dist/cjs",
    "build:es": "BABEL_ENV=es babel src --out-dir dist/es",
    "commonjs:watch": "yarn run build:commonjs -- --watch",
    "es:watch": "yarn run build:es -- --watch",
    "build": "yarn run build:commonjs && yarn run build:es",
    "watch": "yarn run commonjs:watch & yarn run es:watch",
    "dev": "yarn run watch & yarn run lint:watch",
    "lint": "eslint src tests --fix",
    "lint:watch": "nodemon -e js -x 'yarn run lint'",
    "prepare": "yarn run clean && yarn run build"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.5",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-proposal-export-default-from": "^7.16.7",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "babel-plugin-inline-replace-variables": "^1.3.1",
    "babel-plugin-lodash": "^3.3.4",
    "eslint": "^8.9.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.2.1",
    "react-test-renderer": "^17.0.2",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "@loadable/component": "^5.15.2",
    "aphrodite": "^2.3.1",
    "classnames": "^2.2.6",
    "clean-deep": "^3.0.2",
    "core-js": "^3.21.1",
    "escape-regex-string": "^1.0.6",
    "history": "^4.10.1",
    "lodash": "^4.17.20",
    "merge": "^2.1.1",
    "prop-types": "^15.8.1",
    "query-string": "^6.9.0",
    "react-is": "^17.0.2",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.13.9",
    "reselect": "^4.0.0",
    "uuid": "^8.3.2",
    "warning": "^4.0.3"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.2.1"
  }
}
